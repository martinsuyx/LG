#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/bin/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/bin/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/style-dictionary@3.9.2/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../style-dictionary/bin/style-dictionary" "$@"
else
  exec node  "$basedir/../style-dictionary/bin/style-dictionary" "$@"
fi
