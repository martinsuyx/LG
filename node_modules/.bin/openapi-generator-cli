#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules/@openapitools/openapi-generator-cli/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules/@openapitools/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules/@openapitools/openapi-generator-cli/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules/@openapitools/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.24.0_@types+node@24.7.0/node_modules:/home/ubuntu/work/demo-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
else
  exec node  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
fi
